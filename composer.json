{
  "name": "thehostingtool/core",
  "description": "TheHostingTool is a billing system designed for webhosts to be powerfull, customizeable, and secure",
  "type": "symfony-bundle",
  "keywords": [
    "billing",
    "webhost builling",
    "webhosting",
    "webhost",
    "hosting"
  ],
  "homepage": "https://thehostingtool.com",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/TheHostingTool/Core/issues",
    "source": "https://github.com/TheHostingTool/Core",
    "docs": "http://docs.thehostingtool.com"
  },
  "require": {
    "php": ">=7.2",
    "doctrine/dbal": "^2.10",
    "doctrine/doctrine-bundle": "^2.0",
    "doctrine/orm": "^2.7",
    "symfony/config": "^5.0",
    "symfony/console": "^5.0",
    "symfony/event-dispatcher": "^5.1",
    "symfony/event-dispatcher-contracts": "^2.1",
    "symfony/framework-bundle": "^5.0",
    "symfony/security-bundle": "^5.1",
    "symfony/yaml": "^5.0",
    "tightenco/collect": "^7.6",
    "webmozart/path-util": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "TheHostingTool\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
